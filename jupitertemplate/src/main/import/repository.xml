<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <jupitertemplate j:dependencies="bootstrap bootstrap-components default jquery jupiteractualites"
                       j:installedModules="jupitertemplate"
                       j:modulePriority="0"
                       j:moduleType="templatesSet"
                       j:title="JupiterTemplate"
                       jcr:mixinTypes="jmix:hasExternalProviderExtension"
                       jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-jupitertemplate-1.0-SNAPSHOT">
                    <src jcr:primaryType="jnt:folder">
                        <main jcr:primaryType="jnt:folder">
                            <resources jcr:primaryType="jnt:folder">
                                <css jcr:primaryType="jnt:cssFolder">
                                    <fonts.css jcr:primaryType="jnt:cssFile"/>
                                    <footer.css jcr:primaryType="jnt:cssFile"/>
                                    <header.css jcr:primaryType="jnt:cssFile"/>
                                    <Pages.css jcr:primaryType="jnt:cssFile"/>
                                </css>
                                <jnt_Footer jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <Footer.Footer.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_Footer>
                                <jnt_Header jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <Header.Header.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_Header>
                                <jnt_template jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:primaryType="jnt:templateTypeFolder">
                                        <template.jupitertemplate.jsp jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_template>
                            </resources>
                        </main>
                    </src>
                </sources>

            <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder">
                    <bootstrap jcr:primaryType="jnt:folder">
                        <css jcr:primaryType="jnt:folder">
                            <bootstrap.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                            </bootstrap.css>
                        </css>
                    </bootstrap>
                </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder"/>
                    </themes>
                        <img jcr:primaryType="jnt:folder">
                            <Jupiter.jpg j:height="71"
                               j:width="213"
                               j:workInProgress="false"
                               j:xresolution="300 dots"
                               j:yresolution="300 dots"
                               jcr:mixinTypes="jmix:exif jmix:image"
                               jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                            </Jupiter.jpg>
                        </img>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/jupitertemplate/templates/files/themes/default"
                  j:view="jupitertemplate"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                        <home jcr:primaryType="jnt:pageTemplate"/>
                    <simple jcr:primaryType="jnt:pageTemplate"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <header j:view="Header"
                          jcr:mixinTypes="jmix:renderable"
                          jcr:primaryType="jnt:Header"/>
                            <main j:id="main"
                        j:skin="skins.custom"
                        jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                        jcr:primaryType="jnt:area"/>
                            <footer j:view="Footer"
                          jcr:mixinTypes="jmix:renderable"
                          jcr:primaryType="jnt:Footer"/>
                        </pagecontent>
                        <fullnewsarticle j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                            <j:translation_fr jcr:language="fr"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="FullNewsArticle"/>
                            <main jcr:primaryType="jnt:contentList"/>
                        </fullnewsarticle>
                        <fullview j:applyOn=""
                         j:defaultTemplate="false"
                         j:hiddenTemplate="false"
                         jcr:primaryType="jnt:contentTemplate">
                            <j:translation_fr jcr:language="fr"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="FullView"/>
                            <main jcr:primaryType="jnt:contentList"/>
                        </fullview>
                </base>
            </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
        </jupitertemplate>
    </modules>
</content>
