<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <jupitertemplate j:dependencies="bootstrap bootstrap-components default jquery jupiteractualites"
                       j:installedModules="jupitertemplate"
                       j:modulePriority="0"
                       j:moduleType="templatesSet"
                       j:title="JupiterTemplate"
                       jcr:mixinTypes="jmix:hasExternalProviderExtension"
                       jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-jupitertemplate-1.0-SNAPSHOT">
                    <src jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                            <resources jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:folder">
                                <css jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:cssFolder">
                                    <footer.css jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:cssFile"/>
                                    <header.css jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:cssFile"/>
                                    <Pages.css jcr:primaryType="jnt:cssFile"/>
                                </css>
                                <javascript jcr:mixinTypes="jmix:sourceControl"
                                 jcr:primaryType="jnt:javascriptFolder">
                                    <HeaderMenu.js jcr:mixinTypes="jmix:sourceControl"
                                       jcr:primaryType="jnt:javascriptFile"/>
                                </javascript>
                                <jnt_Header jcr:mixinTypes="jmix:sourceControl"
                                 jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:mixinTypes="jmix:sourceControl"
                              jcr:primaryType="jnt:templateTypeFolder">
                                        <Header.Header.jsp jcr:mixinTypes="jmix:sourceControl" jcr:primaryType="jnt:viewFile"/>
                                    </html>
                                </jnt_Header>
                            </resources>
                        </main>
                    </src>
                </sources>

            <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder">
                    <bootstrap jcr:primaryType="jnt:folder">
                        <css jcr:primaryType="jnt:folder">
                            <bootstrap.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                            </bootstrap.css>
                        </css>
                    </bootstrap>
                </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder"/>
                    </themes>
                        <img jcr:primaryType="jnt:folder">
                            <Jupiter.jpg j:height="71"
                               j:width="213"
                               j:workInProgress="false"
                               j:xresolution="300 dots"
                               j:yresolution="300 dots"
                               jcr:mixinTypes="jmix:exif jmix:image"
                               jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                            </Jupiter.jpg>
                        </img>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/jupitertemplate/templates/files/themes/default"
                  j:view="jupitertemplate"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                        <home jcr:primaryType="jnt:pageTemplate"/>
                        <simple jcr:primaryType="jnt:pageTemplate"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <header j:view="Header"
                          jcr:mixinTypes="jmix:renderable"
                          jcr:primaryType="jnt:Header"/>
                            <main j:id="main"
                        j:skin="skins.custom"
                        jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                        jcr:primaryType="jnt:area"/>
                            <footer j:view="Footer"
                          jcr:mixinTypes="jmix:renderable"
                          jcr:primaryType="jnt:Footer"/>
                        </pagecontent>
                            <jupiteractualite j:applyOn="jnt:JupiterActualites"
                                    j:defaultTemplate="true"
                                    j:hiddenTemplate="false"
                                    j:priority="50"
                                 j:view="default"
                                    jcr:primaryType="jnt:contentTemplate">
                            <j:translation_fr j:workInProgress="false"
                                    jcr:language="fr"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="JupiterActualite"/>
                            <main jcr:primaryType="jnt:contentList">
                                <area629577 jcr:primaryType="jnt:area">
                                    <area629577 jcr:primaryType="jnt:contentList">
                                        <main-resource-display j:mainResourceView="fullView" jcr:primaryType="jnt:mainResourceDisplay"/>
                                </area629577>
                                </area629577>
                            </main>
                            </jupiteractualite>
                    </base>
            </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
        </jupitertemplate>
    </modules>
</content>
